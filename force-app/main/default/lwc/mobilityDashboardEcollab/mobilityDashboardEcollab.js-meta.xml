<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Navigate To Selected Dashboard</masterLabel>
    <targets>
        <target>analytics__Dashboard</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="analytics__Dashboard">
           
            <property name="iddash" type="String" label="Id Dashboard"   description="Dashboard da aprire" />
            <property name="nomeDas" type="String" label="Nome Dashboard"   description="Nome della Dashboard" />
            <property name="label" type="String" label="Label" description="Label da visualizzare" default="Vai al Dashboard"/>
            <property name="componentName" type="String" label="Nome Componente" description="Nome Componente da aprire"/>
            <property name="colorelab" type="String" label="Colore label" description="Colore label" default="blue" />
            <property name="font" type="String" label="Font" description="Font del testo :Serif,Arial,Times New Roman,Helvetica, ect" default="Segoe UI"/>
            <property name="fontdim" type="String" label="Dimensione Font" description="Dimensione Font del testo" default="12" />
            <property name="fontstyle" type="String" label="Inserisci Grassetto" description="Inserisci il valore tra Bold, Lighter, Normal o valore numerico per specificare il tipo di grassetto. " default="none" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>