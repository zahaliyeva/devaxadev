                                <div id="container_Collaboratori" class="slds-form--horizontal " style="padding-bottom: 10px;">
                                    <label class="slds-form-element__label " for="Richiedente:">Richiedente:</label>
                                    <div class="slds-form-element__control" >
                                        <!--<ui:inputSelect class="slds-input" aura:id="inputList_collaboratori"  change="{!c.SetCollaboratore}"/> -->
                                        <ui:inputSelect class="slds-input" aura:id="inputList_collaboratori" change="{!c.SetCollaboratore}">
                                            <aura:iteration items="{!v.CollList}" var="level">
                                                <ui:inputSelectOption text="{!level.value}" label="{!level.label}"/>
                                            </aura:iteration>
                                        </ui:inputSelect>
                                    </div>
                                </div>
                            </fieldset>
                            <fieldset class="slds-form--compound slds-col--padded slds-size--1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-3">
                                <div id="container_LOB" class="slds-form--horizontal " style="padding-bottom: 10px;">
                                    <label class="slds-form-element__label " for="LOB:">Area:</label>
                                    <div class="slds-form-element__control" >
                                        <!--<ui:inputSelect class="slds-input" aura:id="inputList_Agencies"  change="{!c.SetAgency}"/> -->
                                        <ui:inputSelect class="slds-input" aura:id="inputLOB"  change="{!c.SetLOB}">
                                            <!--change="{!c.SetLOB}"-->
                                            <!--<aura:iteration items="{!v.AgenciesList}" var="level">
                                 <ui:inputSelectOption text="{!level.value}" label="{!level.label}"/>
                                 </aura:iteration>-->
                                        </ui:inputSelect>
