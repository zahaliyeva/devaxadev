                                            <!--change="{!c.SetAreaMgr}"-->
                                        </ui:inputText>
                                    </div>
                                </div>
                            </fieldset>
                            <fieldset class="slds-form--compound slds-col--padded slds-size--1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-3">
                                <div id="container_Status" class="slds-form--horizontal " style="padding-bottom: 10px;">
                                    <label class="slds-form-element__label " for="Stato:">Stato:</label>
                                    <div class="slds-form-element__control" >
