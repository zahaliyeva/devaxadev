                        <ui:inputSelect class="slds-input" aura:id="inputcategory" disabled="{!v.isDependentDisable}" change="{!c.SetCategory}"  >
                            <!--change="{!c.SetCategory}"disabled="{!v.isDependentDisable}"-->
                            <!--<aura:iteration items="{!v.AgenciesList}" var="level">
                        <ui:inputSelectOption text="{!level.value}" label="{!level.label}"/>
                        </aura:iteration>-->
