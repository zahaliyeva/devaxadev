                            <!--change="{!c.SetAreaMgr}"-->
                        </ui:inputText>
                        
                        <label class="slds-form-element__label " for="Richieste_Risolte">Visualizza anche le richieste risolte</label>
                        <ui:inputCheckbox aura:id="inputText_ClosedStatusSearch" change="{!c.SetClosedStatusSearch}" class="displayFlex" />
                    </lightning:layoutItem>
                </lightning:layout>
                <aura:set attribute="else">
                    <div class="slds-align--absolute-center custom_container_filtri">
                        <div class="slds-grid slds-wrap slds-grid--pull-padded " >
                            <fieldset class="slds-form--compound slds-col--padded slds-size--1-of-1 slds-small-size--1-of-1 slds-medium-size--1-of-1 slds-large-size--1-of-3">
                                <div id="container_Oggetto" class="slds-form--horizontal " style="padding-bottom: 10px;">
                                    <label class="slds-form-element__label " for="Oggetto:">Oggetto:</label>
                                    <div class="slds-form-element__control" >
