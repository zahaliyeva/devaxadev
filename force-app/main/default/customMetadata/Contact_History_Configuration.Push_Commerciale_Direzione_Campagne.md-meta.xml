    <values>
        <field>Query__c</field>
        <value xsi:type="xsd:string">Select Id, Communication_Description__c, Send_Date__c, AccountId__c, MC_Email_Opened__c, MC_Email_Opened_Date__c, MC_Email_Clicked__c, MC_Email_Clicked_Date__c, Status__c 
FROM Communications_to_customer__c where AccountId__c IN :§accountIds§ 
AND Recordtype.DeveloperName = &apos;Standard_Communication&apos; 
AND Notification_Typology__c = &apos;Email&apos; 
AND Status__c not in (&apos;Cliente in Blacklist&apos;) 
AND Process__c != &apos;Agente&apos; 
AND campaign__c = null 
AND Iniziativa_Marketing_Id__r.Base_Giuridica__c = &apos;Contrattuale&apos; 
AND Iniziativa_Marketing_Id__r.Tipologia_Campagna__c = &apos;Commerciale&apos;</value>
    </values>
